<p id="notice"><%= notice %></p>

<h1>Organizations</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Nickname</th>
      <th>Creation date</th>
      <th>Address</th>
      <th>Address complement</th>
      <th>Zip code</th>
      <th>City</th>
      <th>Email</th>
      <th>Phone number</th>
      <th>Status</th>
      <th>Siren</th>
      <th>Description</th>
      <th>Activity sector</th>
      <th>Naf ape</th>
      <th>Logo url</th>
      <th>Website url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @organizations.each do |organization| %>
      <tr>
        <td><%= organization.name %></td>
        <td><%= organization.nickname %></td>
        <td><%= organization.creation_date %></td>
        <td><%= organization.address %></td>
        <td><%= organization.address_complement %></td>
        <td><%= organization.zip_code %></td>
        <td><%= organization.city_id %></td>
        <td><%= organization.email %></td>
        <td><%= organization.phone_number %></td>
        <td><%= organization.status_id %></td>
        <td><%= organization.siren %></td>
        <td><%= organization.description %></td>
        <td><%= organization.activity_sector_id %></td>
        <td><%= organization.naf_ape %></td>
        <td><%= organization.logo_url %></td>
        <td><%= organization.website_url %></td>
        <td><%= link_to 'Show', organization %></td>
        <td><%= link_to 'Edit', edit_organization_path(organization) %></td>
        <td><%= link_to 'Destroy', organization, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

