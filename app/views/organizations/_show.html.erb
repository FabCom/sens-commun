<%card = {
  header: "ORGANISATION", 
  title: @organization.nickname, 
  subtitle: "Créée le #{format_date(@organization.creation_date)}", 
  svg_text: @organization.name, 
  text: @organization.description,
  list: [{label: "List-label", data: "List-data"},{label: "List-label", data: "List-data", type:"primary"}],
  actions: [{label: "Demander à être partie prenante", path: user_dashboards_organizations_path, type:"success"}],
  footer: "Bas de carte"
}%>
<div class="d-flex" style="width:100%">
  <%= render "partials/shared/components/generic_card", card: card %>
</div>

<p>
  <strong>Address:</strong>
  <%= @organization.address %>
</p>

<p>
  <strong>Address complement:</strong>
  <%= @organization.address_complement %>
</p>

<p>
  <strong>Zip code:</strong>
  <%= @organization.zip_code %>
</p>

<p>
  <strong>City:</strong>
  <%= @organization.city_id %>
</p>

<p>
  <strong>Email:</strong>
  <%= @organization.email %>
</p>

<p>
  <strong>Phone number:</strong>
  <%= @organization.phone_number %>
</p>

<p>
  <strong>Status:</strong>
  <%= @organization.status_id %>
</p>

<p>
  <strong>Siren:</strong>
  <%= @organization.siren %>
</p>

<p>
  <strong>Activity sector:</strong>
  <%= @organization.activity_sector_id %>
</p>

<p>
  <strong>Naf ape:</strong>
  <%= @organization.naf_ape %>
</p>

<p>
  <strong>Logo url:</strong>
  <%= @organization.logo_url %>
</p>

<p>
  <strong>Website url:</strong>
  <%= @organization.website_url %>
</p>
