<p id="notice"><%= notice %></p>

<h1>Activity Sectors</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @activity_sectors.each do |activity_sector| %>
      <tr>
        <td><%= activity_sector.name %></td>
        <td><%= link_to 'Show', activity_sector %></td>
        <td><%= link_to 'Edit', edit_activity_sector_path(activity_sector) %></td>
        <td><%= link_to 'Destroy', activity_sector, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Activity Sector', new_activity_sector_path %>
