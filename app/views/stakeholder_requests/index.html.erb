<p id="notice"><%= notice %></p>

<h1>Stakeholder Requests</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Organization</th>
      <th>Message</th>
      <th>Validation</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @stakeholder_requests.each do |stakeholder_request| %>
      <tr>
        <td><%= stakeholder_request.user_id %></td>
        <td><%= stakeholder_request.organization_id %></td>
        <td><%= stakeholder_request.message %></td>
        <td><%= stakeholder_request.validation %></td>
        <td><%= link_to 'Show', stakeholder_request %></td>
        <td><%= link_to 'Edit', edit_stakeholder_request_path(stakeholder_request) %></td>
        <td><%= link_to 'Destroy', stakeholder_request, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Stakeholder Request', new_stakeholder_request_path %>
